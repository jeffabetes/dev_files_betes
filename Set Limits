upper_limit = 91
lower_limit = 24
outlier = False
number = 8

if number > upper_limit:
  outlier = True
if number < lower_limit:
  outlier = True
if outlier == True:
  print(f"{number} is an outlier")

minimum_points = 100
data_points = 150

if data_points >= minimum_points:
  print("There are enough data points!")
if data_points < minimum_points:
  print("Keep collecting data.")
  

